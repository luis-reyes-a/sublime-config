 to get something off the ground here\n**\n** Here are things I want to do with the 2d tile map grid\n** - check against AABB's for wall collisions (and be able to get back the actual specific Tile hit)\n** - check if a floor is being occupied by a wall above it (related to above point)\n**\n**\n*/\n\n#if 0\nvoid update_floor_tiles_for_wall_occupiers(World_Chunk *chunk) {\n    \n    // TODO this loop doesn't have to run first time through (on game reset)\n    for (array_forward(floor, room->layers[TILE_LAYER_GROUND].tiles)) {\n        floor->occupied_by_wall_count = 0;\n    }\n    Tile_Layer *wall_layer   = &room->layers[TILE_LAYER_WALLS];\n    Tile_Layer *ground_layer = &room->layers[TILE_LAYER_GROUND];\n    for (array_forward(wall, wall_layer->tiles)) {\n        Rect2f wall_rect = r2f(wall->pos, .8f, .8f); \n        for (array_forward(floor, ground_layer->tiles)) {\n            Rect2f floor_rect = r2f(floor->pos, .8f, .8f);\n            if (do_overlap(wall_rect, floor_rect)) {\n                floor->occupied_by_wall_count += 1;\n            } \n        } \n    }  \n}\n#endif\n\nvoid register_tile(World_Chunk *chunk, Tile_Layer *layer, Tile *tile, s32 tiles_indices_index) {\n    assert ((tiles_indices_index >= 0) && (tiles_indices_index < countof(layer->tile_indices_plus_one)));\n    assert (layer->tiles.count <= S16_MAX);\n    s32 tile_index_in_array = layer->tiles.get_index(tile);\n    layer->tile_indices_plus_one[tiles_indices_index] = (u8)tile_index_in_array + 1;\n    \n    tile->flags |= TILE_IS_ACTIVE;\n    tile->flags &= ~TILE_FLAGS_TO_CLEAR_ON_REGISTER;\n} \n\nvoid register_tile(World_Chunk *chunk, Tile_Layer *layer, Tile *tile) {\n    // Vector2i chunk_min = get_chunk_min_tile_pos(chunk->chunk_pos.xy);\n    // s32 x_index = tile->pos.x - chunk_min.x; \n    // s32 y_index = tile->pos.y - chunk_min.y;\n    s32 x_index = tile->relative_x + CHUNK_W/2;\n    s32 y_index = tile->relative_y + CHUNK_H/2;\n    \n    \n    assert ((x_index >= 0) && (x_index < CHUNK_