);\n    \n    \n    String exe_path = os_get_exe_path();\n    String levels_path = remove_current_directory(remove_current_directory(exe_path));\n    _game_->levels_directory_path = aprintf(&_game_->arena, \"%.*s%s\", levels_path.length, levels_path.str, \"levels\\\\\");\n    if (!os_create_directory(_game_->levels_directory_path)) {\n        breakpoint;\n    } \n}\n\nvoid on_begin_playing_level() {\n    // @TODO put this elsewhere\n    Game *game = get_game();\n    game->cam.view_height = CHUNK_H; \n    game->cam.view_width  = game->cam.view_height*SCREEN_ASPECT_RATIO;\n    game->move_camera_to_target_t = 1.0f;\n    \n    game->particle_system.init();\n    \n    Level *level = &game->level;\n    Player *player = (Player *)find_entity_by_id(level, level->player_id);\n    if (player) {\n        // @TODO should do something smarter\n        Vector2i chunk_pos = world_to_chunk_pos(player->pos);\n        \n        Vector2 pos = chunk_to_world_pos(chunk_pos);\n        game->init_camera_pos = game->target_camera_pos = pos;\n        game->move_camera_to_target_t = 1.0f;\n    } \n} \n\n\n\n\n\n\n\n\n\n \n\n \n\n/*\nstruct Room {\n    Bucket_Array \n    s32 chunk_indices;  // not serialized\n    Vector2 min_chunk_xy;\n    Vector2 max_chunk_xy;\n    s32 z;\n};\n*/\n\n \n\n\n\nArray_View<Vector2i> get_tiles_near_line(Memory_Arena *arena, Vector2i p1, Vector2i p2, f32 dist) {\n\n    Rect2i bounds;\n    bounds.min.x = MIN(p1.x, p2.x);\n    bounds.min.y = MIN(p1.y, p2.y);\n    bounds.max.x = MAX(p1.x, p2.x);\n    bounds.max.y = MAX(p1.y, p2.y);\n\n    Vector2i dim = v2i(bounds.max.x - bounds.min.x + 1, bounds.max.y - bounds.min.y + 1);\n    if (dim.x < dist) {\n        f32 half_expand = .5f*(dist - dim.x);\n        f32 xmin = bounds.min.x - half_expand;\n        f32 xmax = bounds.max.x + half_expand;\n        bounds.min.x = floor_s32(xmin);\n        bounds.max.x = ceil_s32(xmax);\n    }\n    if (dim.y < dist) {\n        f32 half_expand = .5f*(dist - dim.y);\n        f32 ymin = bounds.min.y - half_expand;\n        f32 ymax = bounds.max.y + half_expand;\n        bounds.min.y = floor_s32(ymin);\n        bounds.max.y = ceil_s32(ymax);\n    }\n\n    dim = v2i(bounds.max.x - bounds.min.x + 1, bounds.max.y - bounds.min.y + 1);\n\n    s32 tile_area  = dim.x*dim.y+1;\n    Array<Vector2i> tiles;\n    init (&tiles, get_temp_memory(), tile_area);\n\n    if (p1 == p2) {\n        for (s32 x = bounds.min.x; x <= bounds.max.x; x += 1) {\n            for (s32 y = bounds.min.y; y <= bounds.max.y; y += 1) {\n                tiles.add(v2i(x, y));\n            }\n        }\n        return tiles.to_view();\n    } \n\n          \n    Vector2 dir = normalize(v2(p2.x - p1.x, p2.y - p1.y));\n    Vector2 normal = perp(dir);\n\n    for (s32 x = bounds.min.x; x <= bounds.max.x; x += 1) {\n        for (s32 y = bounds.min.y; y <= bounds.max.y; y += 1) {\n            Vector2 pos = v2(x, y);\n            Vector2 start_to_pos = v2(pos.x - p1.x, pos.y - p1.y);\n\n            f32 dist_to_line = absolute_value(dot2(normal, start_to_pos));\n            if (dist_to_line < dist) {\n                tiles.add(v2i(x, y));\n            } \n        }\n    }\n    return tiles.to_view();\n}\n\nArea *find_area_by_id(Level *level, u32 area_id) {\n    if (area_id == 0) return nullptr;\n    for (array_forward(area, level->areas)) {\n        if (area->index_plus_one == area_id) return area;\n    }\n    return nullptr;\n}\n\n\n\n\nvoid simulate_level(Game *game, f32 dt) {\n    Level *level = &game->level;\n\n    \n    Area *player_area = find_area_by_id(level, level->player_area_id);\n    \n    \n\n    f32 camera_time_to_reach_target = 1.0f;\n    game->move_camera_to_target_t = MIN(game->move_camera_to_target_t + dt, camera_time_to_reach_target);\n    f32 camera_move_t = game->move_camera_to_target_t / camera_time_to_reach_target;\n    game->cam.pos = lerp(game->init_camera_pos, game->target_camera_pos, camera_move_t);\n    \n    \n    game->move_camera_to_target_t += dt;\n\n    bool simulate_entities = (game->move_camera_to_target_t >= 1.0f);\n    \n\n    #if 0\n    level->is_transitioning_room_last_update = false;\n    if (level->room_transition_t > 0) {\n        level->room_transition_t = MAX(0, level->room_transition_t - dt);\n        f32 t = level->room_transition_t / ADJACENT_ROOM_TRANSITION_MAX_T;\n\n        World_Chunk *prev_room = find_chunk_at_chunk_pos(level, level->prev_player_room_pos);\n        Vector2 prev_room_game_pos = v2(WORLD_CHUNK_TILE_SIZE*prev_room->chunk_pos);\n        Vector2 next_room_game_pos = v2(WORLD_CHUNK_TILE_SIZE*player_room->chunk_pos);\n        game->cam.pos = lerp(prev_room_game_pos, next_room_game_pos, 1-t);\n        \n        if (level->room_transition_t > 0) {\n            simulate_entities = false;\n            level->is_transitioning_room_last_update = true;\n        } \n    } \n    #endif\n    \n    if (simulate_entities && player_area) {\n        simulate_and_move_entities(game, level, player_area, dt);\n    } \n} \n\n\nvoid simulate_game(f32 dt) {\n    Game *game = get_game();\n    \n\n    if (game->do_simulate_level) {\n        simulate_level(game, dt);\n    }\n    game->simulation_index += 1;\n}\n\n\n\n\n\n\n\n// @NOTE this expects cam to have been set already\nvoid draw_level(Level *level, const Render_Camera *cam, s32 chunk_z, f32 dt) {\n    if (!level) {\n        u32 z_order = Z_ORDER_GAME_WORLD;\n        draw_quad(cam->pos, cam->view_width, cam->view_height, RGBA(0,0,0,1), z_order);\n\n        f32 px = 26;\n        String string = \"Call load_level() to load a level\"s;\n        Vector2 pos = cam->pos;\n        pos.x -= .5f*get_text_width_in_world_units(FONT_consola, string, px);\n        draw_string(FONT_consola, px, pos, string, RGBA(1,1,1,1), z_order+1);\n        return;\n    }\n    \n\n    \n    Rect2f camera_bounds = get_camera_bounds((Render_Camera *)cam);\n\n    \n    Vector2i min_camera_chunk_pos = world_to_chunk_pos(camera_bounds.min);\n    Vector2i max_camera_chunk_pos = world_to_chunk_pos(camera_bounds.max);\n\n    s32 num_chunks_visible_x = (max_camera_chunk_pos.x-min_camera_chunk_pos.x + 1);\n    s32 num_chunks_visible_y = (max_camera_chunk_pos.x-min_camera_chunk_pos.y + 1);\n    s32 max_visible_area_count = (num_chunks_visible_x*num_chunks_visible_y) * 4;\n\n    // instead of doing this we could just store a HAS_BEEN_DRAWN flag that gets cleared here\n    // and we iterate over chunks and their areas again and draw if flag not set\n    Array <s32> areas_visible;\n    init (&areas_visible, get_temp_memory(), max_visible_area_count);\n    \n    for (s32 chunk_x = min_camera_chunk_pos.x; chunk_x <= max_camera_chunk_pos.x; chunk_x += 1) {\n        for (s32 chunk_y = min_camera_chunk_pos.y; chunk_y <= max_camera_chunk_pos.y; chunk_y += 1) {\n            World_Chunk *chunk = find_chunk_at_chunk_pos(level, v3i(chunk_x, chunk_y, chunk_z));\n            if (!chunk) continue;\n\n            if (!areas_visible.find_by_value(chunk->area_id)) {\n                areas_visible.add(chunk->area_id);\n            }\n\n            for (u32 layer_id = 0; layer_id < TILE_LAYER_COUNT; layer_id += 1) {\n\n                #if DEVELOPER_BUILD\n                bool editor_should_draw_tile_layer(u32 id);\n                if (!editor_should_draw_tile_layer(layer_id)) continue;\n\n                #endif\n\n                Tile_Layer *layer = chunk->layers + layer_id;\n                for (array_forward(tile, layer->tiles)) {\n                    if (tile->flags & TILE_IS_ACTIVE) {\n                        f32 y  = get_world_pos(layer, tile).y;\n                        f32 zt = (camera_bounds.max.y - y) / cam->view_height;\n                        draw_tile(layer, tile, zt*100);\n                    } \n                } \n            } \n        }\n    }\n\n    for (s32 area_id_index = 0; area_id_index < areas_visible.count; area_id_index += 1) {\n        Area *area = find_area_by_id(level, areas_visible.e[area_id_index]);\n        if (!area || (area->z != chunk_z)) continue;\n\n        FOREACH_ACTIVE_ENTITY (ent, area) {\n            // f32 relative_z_t = relative_z / (f32)(WORLD_CHUNK_TILE_SIZE); \n            // args.z_based_on_ypos = (s32)(relative_z_t*64); // how good should the resolution be?\n            draw_entity(ent, camera_bounds, dt);\n\n            // Collider c = get_collider_for(ent);\n            // draw_bounding_box(c.local_pos, c.w, c.h, 0.03f, RGBA(1,1,1,1));\n            //draw_approximate_circle_outline(col_pos, ent->col.w, 0.03f, RGBA(1,1,1,1), 12);\n        }\n    }\n\n     \n\n    \n    #if 0\n    // f32 aspect_ratio = 16.0f / 9.0f;\n    f32 margin_width = cam->view_width - WORLD_CHUNK_TILE_SIZE;\n    // f32 margin_width = 3.6666666666666666666666666666667f; // this will never change\n    \n    Rect2f margin_rect;\n    margin_rect.max.x = cam->pos.x + .5f*cam->view_width;\n    margin_rect.min.x = margin_rect.max.x - margin_width;\n    margin_rect.min.y = cam->pos.y - .5f*cam->view_height;\n    margin_rect.max.y = margin_rect.min.y + cam->view_height;\n    draw_quad(margin_rect, RGBA(1,1,1,1), Z_ORDER_MARGIN);\n    \n    \n    f32 selector_pad = .15f;\n    f32 selector_size = (margin_width - (selector_count+1)*selector_pad) / selector_count;\n    Vector2 selector_pos = {margin_rect.min.x + selector_pad + .5f*selector_size,\n        /**/                margin_rect.max.y - selector_pad - .5f*selector_size};\n     \n    #endif\n\n\n}\n\nvoid draw_game(f32 dt) {\n    Game *game = get_game();\n    set_active_camera(&game->cam);\n    draw_level(&game->level, &game->cam, game->level.player_z, dt);\n\n    // @TODO this will have to change for when the pause menu or something is stopping the game\n    for (s32 i = game->entity_emitters.count-1; i >= 0; i -= 1) {\n        auto emitter = game->entity_emitters.e + i;\n        if (emitter->simulated_this_frame) {\n            // to get ready for next frame\n            emitter->simulated_this_frame = false;\n        } else {\n            bool did_free = game->particle_system.free_particle_emitter(emitter->emitter);\n            assert (did_free);\n            game->entity_emitters.remove_unordered(i);\n        } \n    }\n}\n\n\n",
